							<xs:sequence>
								<xs:element name="value" maxOccurs="unbounded">
									<xs:complexType>
										<xs:simpleContent>
											<xs:extension base="D2LogicalModel:MultiLingualString">
												<xs:attribute name="lang" type="xs:language"/>
											</xs:extension>
										</xs:simpleContent>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
