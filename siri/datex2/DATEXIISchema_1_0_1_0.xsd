								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="referencePointDistance" type="D2LogicalModel:MetresAsFloat" minOccurs="0">
