			<xs:element name="elaboratedData" type="D2LogicalModel:ElaboratedData" minOccurs="0"/>
			<xs:element name="cctvimages" type="D2LogicalModel:CCTVImages" minOccurs="0"/>
			<xs:element name="trafficViewRecordExtension" type="D2LogicalModel:ExtensionType" minOccurs="0"/>
